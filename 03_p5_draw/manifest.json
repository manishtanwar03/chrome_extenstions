{
    "manifest_version": 2,
    "name": "p5_sketch_ext",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "p5.js",
                "p5.dom.js",
                "sketch.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "logo.png"
    }
}